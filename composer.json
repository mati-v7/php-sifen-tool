{
    "name": "nyxcode/php-sifen-tool",
    "description": "A php package to simplify integration with Paraguay's SIFEN API for electronic invoicing.",
    "type": "library",
    "require": {
        "robrichards/xmlseclibs": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "12.1.x",
        "phpstan/phpstan": "^2.1",
        "squizlabs/php_codesniffer": "^4.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nyxcode\\PhpSifenTool\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Nyxcode\\PhpSifenTool\\Tests\\": "tests/"
        }
      },
    "authors": [
        {
            "name": "Matias Vega",
            "email": "matias.vega.m17@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
